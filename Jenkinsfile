pipeline {
    agent any
    stages {
        stage('Clone') {
            steps {
                echo 'Cloning code from GitHub'
            }
        }
        stage('Build') {
            steps {
                echo 'Build started'
            }
        }
        stage('Test') {
            steps {
                echo 'Running tests'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy completed'
            }
        }
    }
}
